{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": [".next/**", "!.next/cache/**"]
      },
      "dev": {
        "persistent": true,
        "cache": false
      },
      "storybook": {
        "persistent": true,
        "cache": false
      },
      "lint": {
        "outputs": ["node_modules/.cache/.eslintcache"]
      }
    }
  }